<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="morpion.MainController">
   <Button fx:id="backToMenu" layoutX="14.0" layoutY="42.0" mnemonicParsing="false" onAction="#backToMenu" text="Back to menu" />
   <MenuBar id="menuBar" layoutY="1.0" prefHeight="26.0" prefWidth="600.0">
      <Menu mnemonicParsing="false" text="Settings">
         <MenuItem mnemonicParsing="false" onAction="#openDifficultySettings" text="Difficulty" />
         <MenuItem mnemonicParsing="false" onAction="#openModelSettings" text="Models" />
      </Menu>
   </MenuBar>
   <TextField fx:id="textName1" layoutX="225.0" layoutY="250.0" />
   <TextField fx:id="textName2" layoutX="225.0" layoutY="358.0" />
   <Label layoutX="268.0" layoutY="216.0" text="Username 1" />
   <Label layoutX="268.0" layoutY="326.0" text="Username 2" />
   <Label layoutX="142.0" layoutY="94.0" text="Choose your names">
      <font>
         <Font size="36.0" />
      </font>
   </Label>
   <Button layoutX="277.0" layoutY="439.0" mnemonicParsing="false" onAction="#getPlayMulti" text="Play" />
   <Label fx:id="labelName1" layoutX="247.0" layoutY="276.0" text="Choose a username" textFill="RED" visible="false" />
   <Label fx:id="labelName2" layoutX="248.0" layoutY="384.0" text="Choose a username" textFill="RED" visible="false" />
</AnchorPane>
